{
  "appId": "com.remindr.app",
  "appName": "Remindr",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "cordova": {},
  "ios": {
    "allowMixedContent": true
  },
  "android": {
    "allowMixedContent": true
  }
}